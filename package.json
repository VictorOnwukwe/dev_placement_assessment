{
  "name": "dev_placement_assessment",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "jest",
    "clear_jest": "jest --clearCache",
    "test:coverage": "npm test -- --coverage"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.vue$": "./node_modules/vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "./src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ]
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "flush-promises": "^1.0.2",
    "vue": "^2.6.11",
    "vue-router": "^3.4.9",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@testing-library/vue": "^5.6.1",
    "@vue/cli-plugin-babel": "^4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.0",
    "@vue/cli-service": "^4.5.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^26.6.3",
    "jest-serializer-vue": "^2.0.2",
    "jest-vue": "^0.8.2",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
